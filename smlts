			'icon'     => 'table',
			'function' => array( 'Shortcodes_Master_Smlts_Shortcodes', 'smlts_pricing_table' ),
			'atts' => array()
		);
		
		/*cols*/
		$shortcodes['smlts_cols'] = array(
			'name'     => __( 'LT columns wrapper', 'smlts' ),
			'type'     => 'wrap',
			'group'    => 'lizatom content',
			'content' =>  __( "Here goes columns of the row.", 'smlts' ),             
			'desc'     => __( 'CSS columns.', 'smlts' ),
			'note'     => sprintf( __( 'Read more on this shortcode %s here %s', 'sm' ), '<a href="https://lizatom.com/wiki/shortcodes-master-lizatom/#lizatom-shortcodes-shortcodes-overview-columns" target="_blank">', '&rarr;</a>' ),
			'icon'     => 'th',
			'function' => array( 'Shortcodes_Master_Smlts_Shortcodes', 'smlts_cols' ),			
		);
		/*col*/		
		$shortcodes['smlts_col'] = array(
			'name'     => __( 'LT column', 'smlts' ),
			'type'     => 'wrap',
			'group'    => 'lizatom content',
			'content' =>  __( "Here goes content of the column.", 'smlts' ),             
			'desc'     => __( 'CSS columns.', 'smlts' ),
			'note'     => sprintf( __( 'Read more on this shortcode %s here %s', 'sm' ), '<a href="https://lizatom.com/wiki/shortcodes-master-lizatom/#lizatom-shortcodes-shortcodes-overview-columns" target="_blank">', '&rarr;</a>' ),
			'icon'     => 'th',
			'function' => array( 'Shortcodes_Master_Smlts_Shortcodes', 'smlts_col' ),
			'atts' => array(
				'type' => array(
					'type' => 'select',
					'values' => array(
						'lts-one-half'   => __( 'one-half', 'smlts' ),
						'lts-one-third'   => __( 'one-third', 'smlts' ),
						'lts-one-fourth'   => __( 'one-fourth', 'smlts' ),
						'lts-one-fifth'   => __( 'one-fifth', 'smlts' ),
						'lts-one-sixth'   => __( 'one-sixth', 'smlts' ),
						'lts-two-third'   => __( 'two-third', 'smlts' ),
						'lts-two-fifth'   => __( 'two-fifth', 'smlts' ),
						'lts-three-fifth'   => __( 'three-fifth', 'smlts' ),
						'lts-four-fifth'   => __( 'four-fifth', 'smlts' ),
						'lts-five-sixth'   => __( 'five-sixth', 'smlts' ),						
						'lts-one-half lts-col-last'   => __( 'one-half last', 'smlts' ),
						'lts-one-third lts-col-last'   => __( 'one-third last', 'smlts' ),
						'lts-one-fourth lts-col-last'   => __( 'one-fourth last', 'smlts' ),
						'lts-one-fifth lts-col-last'   => __( 'one-fifth last', 'smlts' ),
						'lts-one-sixth lts-col-last'   => __( 'one-sixth last', 'smlts' ),
						'lts-two-third lts-col-last'   => __( 'two-third last', 'smlts' ),
						'lts-two-fifth lts-col-last'   => __( 'two-fifth last', 'smlts' ),
						'lts-three-fifth lts-col-last'   => __( 'three-fifth last', 'smlts' ),
						'lts-four-fifth lts-col-last'   => __( 'four-fifth last', 'smlts' ),
						'lts-five-sixth lts-col-last'   => __( 'five-sixth last', 'smlts' ),
					),
					'default' => 'one-half',
					'name' => __( 'Type', 'smlts' ),
					'desc' => __( 'Type of the column.', 'smlts' )
				)
			)
		);
		
		return $shortcodes;

	}

}

