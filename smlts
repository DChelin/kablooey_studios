			'note'     => sprintf( __( 'Read more on this shortcode %s here %s', 'sm' ), '<a href="https://lizatom.com/wiki/shortcodes-master-lizatom/#lizatom-shortcodes-shortcodes-overview-css3-accordion" target="_blank">', '&rarr;</a>' ),
			'icon'     => 'align-justify',
			'function' => array( 'Shortcodes_Master_Smlts_Shortcodes', 'smlts_css3accordion' ),
			'atts' => array(
				'title' => array(
                            'default' => 'Accordion title must be unique',
                            'name' => __( 'CSS3 acordion title', 'smlts' ),
                            'desc' => __( 'Accordion title must be unique', 'smlts' )
						),	

			)
		);
/*css3accordions*/		
		$shortcodes['smlts_css3accordions'] = array(
			'name'     => __( 'LT CSS3 accordions', 'smlts' ),
			'type'     => 'wrap',
			'group'    => 'lizatom box',
			'content' =>  __( htmlspecialchars('[sm_smlts_css3accordion title="Accordion title must be unique1"]<p>test content 1</p>[/sm_smlts_css3accordion][sm_smlts_css3accordion title="Accordion title must be unique2"]<p>test content 2</p>[/sm_smlts_css3accordion][sm_smlts_css3accordion title="Accordion title must be unique3"]<p>test content 3</p>[/sm_smlts_css3accordion]'), 'smlts'),             
			'desc'     => __( 'Beautiful CSS3 accordions.', 'smlts' ),
