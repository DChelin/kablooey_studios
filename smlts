			'icon'     => 'align-justify',
			'function' => array( 'Shortcodes_Master_Smlts_Shortcodes', 'smlts_css3accordions' ),
			'atts' => array(
				'type' => array(
					'type' => 'select',
					'values' => array(
						'lizatom-css3accordion-horizontal'   => __( 'lizatom-css3accordion-horizontal', 'smlts' ),
						'lizatom-css3accordion-vertical'   => __( 'lizatom-css3accordion-vertical', 'smlts' )
					),
					'default' => 'rounded',
					'name' => __( 'Type', 'smlts' ),
					'desc' => __( 'Type of the dropcap.', 'smlts' )
				),
				'color' => array(
                                        'type' => 'color',
                                        'values' => array( ),
                                        'default' => '#000000',
                                        'name' => __( 'Color', 'smlts' ), 
                                        'desc' => __( 'Color of the accordion. Hexadecimal: #ffffff or name: white.', 'smlts' )
                        ),
				'title' => array(
                            'default' => 'Accordion title',
                            'name' => __( 'Title of the accordion', 'smlts' ),
                            'desc' => __( 'Title of the accordion', 'smlts' )
						),	

			)
		);		

/*portfolio*/		
		$shortcodes['smlts_portfolio'] = array(
			'name'     => __( 'LT Portfolio', 'smlts' ),
			'type'     => 'single',
			'group'    => 'lizatom box',
			'content' =>  __('', 'smlts'),             
			'desc'     => __( 'Create filtered portfolio from featured images.', 'smlts' ),
			'note'     => sprintf( __( 'Read more on this shortcode %s here %s', 'sm' ), '<a href="https://lizatom.com/wiki/shortcodes-master-lizatom/#lizatom-shortcodes-shortcodes-overview-animated-portfolio" target="_blank">', '&rarr;</a>' ),
			'icon'     => 'th',
			'function' => array( 'Shortcodes_Master_Smlts_Shortcodes', 'smlts_portfolio' ),
			'atts' => array(
				'id' => array(
                            'default' => 'unique-id-of-portfolio',
                            'name' => __( 'ID', 'smlts' ),
                            'desc' => __( 'Unique identifier of the portfolio.', 'smlts' )
						),
				'animation' => array(
					'type' => 'select',
					'values' => array(
						'1'   => __( '1', 'smlts' ),
						'2'   => __( '2', 'smlts' ),
						'3'   => __( '3', 'smlts' )
					),
					'default' => '1',
					'name' => __( 'Animation', 'smlts' ),
					'desc' => __( 'Choose filtering animation.', 'smlts' )
				),
				'columns' => array(
					'type' => 'select',
					'values' => array(
						'1'   => __( '1', 'smlts' ),
						'2'   => __( '2', 'smlts' ),
						'3'   => __( '3', 'smlts' )
					),
					'default' => '1',
					'name' => __( 'Columns', 'smlts' ),
					'desc' => __( 'Number of columns.', 'smlts' )
				),
				'target' => array(
					'type' => 'select',
					'values' => array(
						'_self'   => __( '_self', 'smlts' ),
						'_blank'   => __( '_blank', 'smlts' )
					),
					'default' => '_self',
					'name' => __( 'Target attribute', 'smlts' ),
					'desc' => __( 'For opening link in new widow, choose _blank.', 'smlts' )
				),
				'categories' => array(
                            'default' => '',
                            'name' => __( 'Categories', 'smlts' ),
                            'desc' => __( 'Categories to choose featured images separated by comma. Example: 1,2,4,6', 'smlts' )
						),	

			)
		);		
/*pricing table*/		
		$shortcodes['smlts_pricing_table'] = array(
			'name'     => __( 'LT pricing table', 'smlts' ),
			'type'     => 'wrap',
			'group'    => 'lizatom box',
			'content' =>  __( htmlspecialchars($table_code), 'smlts' ),             
